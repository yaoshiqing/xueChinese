<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:paddingTop="45dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/ic_search_words_not_list"
        android:drawablePadding="39dp"
        android:text="@string/stringWrongQuestionSetNotData"
        android:textColor="@color/color66"
        android:textSize="15sp"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:layout_marginTop="98dp"/>

    <com.gjjy.basiclib.widget.NetworkErrorView
        android:id="@+id/not_data_nev_network_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

</FrameLayout>