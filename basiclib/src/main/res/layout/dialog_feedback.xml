<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_dialog_def_bg">

    <com.gjjy.basiclib.widget.ContentWebView
        android:id="@+id/dialog_feedback_cwv_web"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/dialog_feedback_iv_close_btn"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@drawable/ic_dialog_free_vouchers_close_btn"
        android:scaleType="centerInside"
        android:contentDescription="@string/stringEmpty"
        android:layout_gravity="end"
        android:padding="12dp"
        android:focusable="true"
        android:clickable="true"/>
</FrameLayout>